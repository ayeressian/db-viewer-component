{
  "name": "db-viewer-component",
  "version": "4.4.0",
  "description": "A web component for showing db schemas",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayeressian/db-viewer-component"
  },
  "keywords": [
    "database",
    "db",
    "schema",
    "viewer"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --watch --config=./webpack.config/webpack.config.ts --host 0.0.0.0",
    "build-prod": "webpack --mode=production --config=./webpack.config/webpack.config.ts",
    "build-dev": "webpack --mode=development --config=./webpack.config/webpack.config.ts",
    "build-dev-watch": "webpack --mode=development --watch --config=./webpack.config/webpack.config.ts",
    "publish-npm": "rm -rf dist && npm run build-prod && npm publish",
    "publish-npm-dev": "rm -rf dist && npm run build-dev && npm publish --tag dev",
    "test": "mocha-webpack \"test/**/*.ts\" --mode development --webpack-config=./webpack.config/webpack.test.config.ts -r jsdom-global/register",
    "test:watch": "npm run test -- --watch",
    "test:debug": "webpack-dev-server --config webpack.config/webpack.test.debug.config.ts --hot --inline",
    "e2e": "npm run e2e-chromium",
    "e2e-chromium": "BROWSER_TYPE=chromium jasmine-ts --config=e2e/jasmine.json",
    "e2e-firefox": "BROWSER_TYPE=firefox jasmine-ts --config=e2e/jasmine.json",
    "e2e-webkit": "BROWSER_TYPE=webkit jasmine-ts --config=e2e/jasmine.json",
    "start-and-e2e": "start-server-and-test start http://localhost:9998 e2e",
    "lint": "npx eslint . --ext .ts"
  },
  "main": "dist/main.js",
  "types": "dist/src/index.d.ts",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/copy-webpack-plugin": "^5.0.2",
    "@types/jasmine": "^3.5.13",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.54",
    "@types/sinon": "^7.5.2",
    "@types/sinon-chai": "^3.2.4",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "acorn": "^7.4.0",
    "babel-eslint": "^8.2.6",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-loader": "^0.5.5",
    "http-server": "^0.12.3",
    "img-loader": "^3.0.0",
    "jasmine": "^3.6.1",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "playwright": "^1.3.0",
    "prettier": "^2.0.5",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0",
    "start-server-and-test": "^1.11.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "url-loader": "^1.1.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "lodash": "^4.17.20"
  }
}
